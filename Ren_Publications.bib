@article{ren_electrochemically_2020,
	title = {Electrochemically {Controlled} {Nucleation} of {Single} {CO2} {Nanobubbles} via {Formate} {Oxidation} at {Pt} {Nanoelectrodes}},
	volume = {11},
	copyright = {All rights reserved},
	issn = {1948-7185 (Electronic) 1948-7185 (Linking)},
	url = {https://doi.org/10.1021/acs.jpclett.9b03898},
	doi = {10.1021/acs.jpclett.9b03898},
	abstract = {CO2 is an anodic product of many liquid fuel cells. The nucleation of CO2 nanobubbles during cell operation may block the transport of the fuel to the anode, lowering the overall conversion efficiency. Herein, we report the controlled formation of individual CO2 nanobubbles at Pt nanoelectrodes via the electrooxidation of formate. The electrochemical data are used to extract key parameters of CO2 gas nucleation. We determine that CO2 bubbles nucleate when the concentration of CO2 at the Pt electrode is greater than approximately 0.6 M, corresponding to supersaturation of approximately 18. The critical nucleus required for the formation of a CO2 bubble is measured to have a radius of curvature of approximately 100 nm, a contact angle of 173 degrees , and contains approximately 70 CO2 molecules.},
	number = {4},
	journal = {The Journal of Physical Chemistry Letters},
	author = {Ren, Hang and Edwards, Martin A. and Wang, Yufei and White, Henry S.},
	month = jan,
	year = {2020},
	pages = {1291--1296},
	file = {2020_Ren_J Phys Chem Lett_Electrochemically Controlled Nucleation of Single CO2 Nanobubbles via Formate.pdf:/Users/Ren/Library/CloudStorage/Box-Box/My Box Folder/9. Reading and references/00_Zotero/Hang Ren Publications/Miami/2020_Ren_J Phys Chem Lett_Electrochemically Controlled Nucleation of Single CO2 Nanobubbles via Formate.pdf:application/pdf},
