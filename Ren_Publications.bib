@article{mondaca-medina_nanoelectrochemistry_2023,
	title = {Nanoelectrochemistry in electrochemical phase transition reactions},
	volume = {14},
	issn = {2041-6520},
	url = {http://dx.doi.org/10.1039/D3SC01857A},
	doi = {10.1039/D3SC01857A},
	abstract = {Electrochemical phase transition is important in a range of processes, including gas generation in fuel cells and electrolyzers, as well as in electrodeposition in battery and metal production. Nucleation is the first step in these phase transition reactions. A deep understanding of the kinetics, and mechanism of the nucleation and the structure of the nuclei and nucleation sites is fundamentally important. In this perspective, theories and methods for studying electrochemical nucleation are briefly reviewed, with an emphasis on nanoelectrochemistry and single-entity electrochemistry approaches. Perspectives on open questions and potential future approaches are also discussed.},
