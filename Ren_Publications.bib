	year = {2024},
	keywords = {NSF},
	pages = {11126--11132},
	file = {Lee and Ren - 2024 - Tuning Electrocatalytic Oxygen Reduction Reaction with Dynamic Control of Electrochemical Interfaces.pdf:/Users/Ren/Library/CloudStorage/Box-Box/My Box Folder/9. Reading and references/00_Zotero/5_Hang Ren Publications/UT/Lee and Ren - 2024 - Tuning Electrocatalytic Oxygen Reduction Reaction with Dynamic Control of Electrochemical Interfaces.pdf:application/pdf},
}

@article{li_stochastic_2022,
